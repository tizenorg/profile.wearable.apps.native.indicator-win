		image : RSSI_IMAGE_DIR"/signal_strength_01.png" COMP;
		image : RSSI_IMAGE_DIR"/signal_strength_02.png" COMP;
		image : RSSI_IMAGE_DIR"/signal_strength_03.png" COMP;
		image : RSSI_IMAGE_DIR"/signal_strength_04.png" COMP;

		image : RSSI_IMAGE_DIR"/roaming_strength_00.png" COMP;
		image : RSSI_IMAGE_DIR"/roaming_strength_01.png" COMP;
		image : RSSI_IMAGE_DIR"/roaming_strength_02.png" COMP;
		image : RSSI_IMAGE_DIR"/roaming_strength_03.png" COMP;
		image : RSSI_IMAGE_DIR"/roaming_strength_04.png" COMP;
		image : indicator/indi_flight_mode.png COMP;
		image : indicator/indi_no_signal.png COMP;
		image : Indicator_business/stat_sys_data_unknown.png COMP;
		
	}
	parts {
		part {
			name : "img.rssi";
			type : IMAGE;
			scale : 1;
			description {
				state : "default" 0.0;
				visible : 1;
				min : DYNAMIC_ICON_WIDTH DYNAMIC_ICON_HEIGHT;
				max : DYNAMIC_ICON_WIDTH DYNAMIC_ICON_HEIGHT;
				fixed : 1 1;
				image.normal : "indicator/indi_no_signal.png";
				//image.normal : RSSI_IMAGE_DIR"/signal_strength_03.png";
				color_class : "AO016";
			}
			DESCRIPTION_RSSI(00);
			DESCRIPTION_RSSI(01);
			DESCRIPTION_RSSI(02);
			DESCRIPTION_RSSI(03);
			DESCRIPTION_RSSI(04);
			DESCRIPTION_RSSI_SPRINT(0);
			DESCRIPTION_RSSI_SPRINT(1);
			DESCRIPTION_RSSI_SPRINT(2);
			DESCRIPTION_RSSI_SPRINT(3);
			DESCRIPTION_RSSI_SPRINT(4);
			DESCRIPTION_RSSI_VERIZON(0);
			DESCRIPTION_RSSI_VERIZON(1);
			DESCRIPTION_RSSI_VERIZON(2);
			DESCRIPTION_RSSI_VERIZON(3);
			DESCRIPTION_RSSI_VERIZON(4);
			DESCRIPTION_RSSI_VERIZON(5);

			DESCRIPTION_ROAMING_RSSI(00);
			DESCRIPTION_ROAMING_RSSI(01);
			DESCRIPTION_ROAMING_RSSI(02);
			DESCRIPTION_ROAMING_RSSI(03);
			DESCRIPTION_ROAMING_RSSI(04);

			/* No signal */
			description {
				state : "state_No_signal";
				inherit : "default" 0.0;
			}
			description {
				state : "state_sprint_No_signal";
				inherit : "default" 0.0;
				image.normal : "Indicator_business/Roaming_RSSI/RSSI_Sprint/stat_sys_nosignal.png";
			}
			description {
				state : "state_verizon_No_signal";
				inherit : "default" 0.0;
				image.normal : "Indicator_business/stat_sys_data_vzw_unknown.png";
			}
			description {
				state : "state_tmo_No_signal";
				inherit : "default" 0.0;
				image.normal : "Indicator_business/Roaming_RSSI/RSSI/TMO/tw_stat_sys_5_level_no_svc.png";
			}

			description {
				state : "state_Flight_mode";
				inherit : "default" 0.0;
				image.normal : "indicator/indi_flight_mode.png";
			}
			description {
				state : "state_none";
				inherit : "default" 0.0;
				visible: 0;
			}
		}
		/* for Sprint */
		part {
			name : "img.roaming";
			type : IMAGE;
			scale : 1;
			description {
				state : "default" 0.0;
				visible : 0;
				min : DYNAMIC_ICON_WIDTH DYNAMIC_ICON_HEIGHT;
				max : DYNAMIC_ICON_WIDTH DYNAMIC_ICON_HEIGHT;
				fixed : 1 1;
				image.normal : "Indicator_business/Roaming_RSSI/RSSI_Sprint/SPR_network/stat_sys_data_connected_roam.png";
				color_class : "AO016";
			}
			description {
				state : "show";
				inherit : "default" 0.0;
				visible: 1;
			}
		}
		/* for Verizon */
		part {
			name : "img.delta.roaming";
			type : IMAGE;
			scale : 1;
			description {
				state : "default" 0.0;
				visible : 0;
				min : DYNAMIC_ICON_WIDTH DYNAMIC_ICON_HEIGHT;
				max : DYNAMIC_ICON_WIDTH DYNAMIC_ICON_HEIGHT;
				fixed : 1 1;
				image.normal : "Indicator_business/Roaming_RSSI/Roaming/stat_sys_data_connected_cdma_roam.png";
				color_class : "AO016";
			}
			description {
				state : "show";
				inherit : "default" 0.0;
				visible: 1;
			}
		}
	}

	programs {
		/* for Sprint */
		program {
			name : "set_rssi_roaming_show";
			signal : "set_rssi_roaming_show";
			source : "img.roaming";
			action : STATE_SET "show" 0.0;
			target : "img.roaming";
		}
		program {
			name : "set_rssi_roaming_hide";
			signal : "set_rssi_roaming_hide";
			source : "img.roaming";
			action : STATE_SET "default" 0.0;
			target : "img.roaming";
		}
		/* for Verizon */
		program {
			name : "set_delta_roaming_on";
			signal : "set_delta_roaming_on";
			source : "img.delta.roaming";
			action : STATE_SET "show" 0.0;
			target : "img.delta.roaming";
		}
		program {
			name : "set_delta_roaming_off";
			signal : "set_delta_roaming_off";
			source : "img.delta.roaming";
			action : STATE_SET "default" 0.0;
			target : "img.delta.roaming";
		}
		PROGRAM_RSSI(00);
		PROGRAM_RSSI(01);
		PROGRAM_RSSI(02);
		PROGRAM_RSSI(03);
		PROGRAM_RSSI(04);

		PROGRAM_RSSI_SPRINT(0);
		PROGRAM_RSSI_SPRINT(1);
		PROGRAM_RSSI_SPRINT(2);
		PROGRAM_RSSI_SPRINT(3);
		PROGRAM_RSSI_SPRINT(4);

		PROGRAM_RSSI_VERIZON(0);
		PROGRAM_RSSI_VERIZON(1);
		PROGRAM_RSSI_VERIZON(2);
		PROGRAM_RSSI_VERIZON(3);
		PROGRAM_RSSI_VERIZON(4);
		PROGRAM_RSSI_VERIZON(5);

		PROGRAM_ROAMING_RSSI(00);
		PROGRAM_ROAMING_RSSI(01);
		PROGRAM_ROAMING_RSSI(02);
		PROGRAM_ROAMING_RSSI(03);
		PROGRAM_ROAMING_RSSI(04);
		program {
			name : "set_rssi_No_signal";
			signal : "set_rssi_No_signal";
			source : "img.rssi";
			action : STATE_SET "state_No_signal" 0.0;
			target : "img.rssi";
		}
		program {
			name : "set_rssi_sprint_No_signal";
