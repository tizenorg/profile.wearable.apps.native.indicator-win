		image : "CSC/Roaming_ctc/roaming_ctc_4_level_00.png" COMP;
		image : "CSC/Roaming_ctc/roaming_ctc_4_level_01.png" COMP;
		image : "CSC/Roaming_ctc/roaming_ctc_4_level_02.png" COMP;
		image : "CSC/Roaming_ctc/roaming_ctc_4_level_03.png" COMP;
		image : "CSC/Roaming_ctc/roaming_ctc_4_level_04.png" COMP;

