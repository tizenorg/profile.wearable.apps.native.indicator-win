			source : "img.rssi";
			action : STATE_SET "state_Flight_mode" 0.0;
			target : "img.rssi";
		}
		program {
			name : "set_rssi_none";
			signal : "set_rssi_none";
			source : "img.rssi";
			action : STATE_SET "state_none" 0.0;
			target : "img.rssi";
		}
	}
