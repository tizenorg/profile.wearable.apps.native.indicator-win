			target : "img.rssi";
		}
		program {
			name : "set_rssi_verizon_No_signal";
			signal : "set_rssi_verizon_No_signal";
			source : "img.rssi";
			action : STATE_SET "state_verizon_No_signal" 0.0;
			target : "img.rssi";
		}
		program {
			name : "set_rssi_tmo_No_signal";
