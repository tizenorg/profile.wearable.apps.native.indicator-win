#define DESCRIPTION_RSSI(num)\
	description {\
		state : "state_"num;\
		inherit : "default" 0.0;\
		image.normal : RSSI_IMAGE_DIR"/signal_strength_"num".png";\
	}\

#define DESCRIPTION_RSSI_SPRINT(num)\
	description {\
		state : "state_sprint_"num;\
		inherit : "default" 0.0;\
		image.normal : "Indicator_business/Roaming_RSSI/RSSI_Sprint/stat_sys_signal_"num".png";\
	}\

#define DESCRIPTION_RSSI_VERIZON(num)\
	description {\
		state : "state_verizon_"num;\
		inherit : "default" 0.0;\
		image.normal : "Indicator_business/Roaming_RSSI/RSSI_vzw/tw_stat_sys_5_level_signal_"num".png";\
	}\

#define DESCRIPTION_ROAMING_RSSI(num)\
	description {\
		state : "state_roaming_"num;\
		inherit : "default" 0.0;\
		image.normal : RSSI_IMAGE_DIR"/roaming_strength_"num".png";\
	}\

#define DESCRIPTION_ROAMING_RSSI_CTC(num)\
	description {\
		state : "state_roaming_ctc_"num;\
		inherit : "default" 0.0;\
		image.normal : "CTC/Roaming_ctc/roaming_ctc_4_level_"num".png";\
	}\


#define PROGRAM_RSSI(num)\
	program {\
		name : "set_rssi_"num;\
		signal : "set_rssi_"num;\
		source : "img.rssi";\
		action : STATE_SET "state_"num 0.0;\
		target : "img.rssi";\
	}\

#define PROGRAM_RSSI_SPRINT(num)\
	program {\
		name : "set_rssi_sprint_"num;\
		signal : "set_rssi_sprint_"num;\
		source : "img.rssi";\
		action : STATE_SET "state_sprint_"num 0.0;\
		target : "img.rssi";\
	}\

#define PROGRAM_RSSI_VERIZON(num)\
	program {\
		name : "set_rssi_verizon_"num;\
		signal : "set_rssi_verizon_"num;\
		source : "img.rssi";\
		action : STATE_SET "state_verizon_"num 0.0;\
		target : "img.rssi";\
	}\


#define PROGRAM_ROAMING_RSSI(num)\
	program {\
		name : "set_roaming_rssi_"num;\
		signal : "set_roaming_rssi_"num;\
		source : "img.rssi";\
		action : STATE_SET "state_roaming_"num 0.0;\
		target : "img.rssi";\
	}\

#define PROGRAM_ROAMING_RSSI_CTC(num)\
	program {\
		name : "set_roaming_rssi_ctc_"num;\
		signal : "set_roaming_rssi_ctc_"num;\
		source : "img.rssi";\
		action : STATE_SET "state_roaming_ctc_"num 0.0;\
		target : "img.rssi";\
	}\

group {
