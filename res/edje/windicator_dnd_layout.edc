		/* VZW */
		program {
			name: "dnd.vzw";
			signal: "dnd.vzw";
			source: "img.disturb.icon";
			action: STATE_SET "vzw" 0.0;
			target: "img.disturb.icon";
		}

		// Enable
		program {
			name: "dnd.enable";
			signal: "dnd.enable";
			source: "dnd.bg.icon";
			action: STATE_SET "default" 0.0;
		//	target: "img.disturb.bg";
		//	target: "img.disturb.icon";
			target: "img.disturb.bar";
		}
		// Disable
		program {
			name: "dnd.disable";
			signal: "dnd.disable";
			source: "dnd.bg.icon";
			action: STATE_SET "off" 0.0;
		//	target: "img.disturb.bg";
		//	target: "img.disturb.icon";
			target: "img.disturb.bar";
		}

		/* Press effect */
		program {
			name: "dnd.pressed";
			signal: "dnd.pressed";
			source: "img.disturb.bg";
			action: STATE_SET "selected" 0.0;
			target: "img.disturb.bg";
		}
		program {
			name: "dnd.released";
			signal: "dnd.released";
			source: "img.disturb.bg";
			action: STATE_SET "default" 0.0;
			target: "img.disturb.bg";
		}
	}
}