				state : "default" 0.0;
				min : DND_TEXT_WIDTH DND_TEXT_HEIGHT;
				visible : 1;
				fixed : 1 1;
				align : 0.5 0.0;
				rel1 { relative : 0.5 1.0; to : "padding.top"; }
				rel2 { relative : 0.5 1.0; to : "padding.top"; }
